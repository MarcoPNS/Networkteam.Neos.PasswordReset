<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
	<file original="" product-name="Networkteam.Neos.PasswordReset" source-language="en" target-language="de" datatype="plaintext">
		<body>
			<trans-unit id="mail.noAccountSubject" xml:space="preserve">
				<source>Lost password</source>
				<target>Passwort vergessen</target>
			</trans-unit>
			<trans-unit id="mail.forgotPasswordSubject" xml:space="preserve">
				<source>Lost password</source>
				<target>Passwort vergessen</target>
			</trans-unit>

            <trans-unit id="passwordManagement.change.success.title" xml:space="preserve">
				<source>Password changed</source>
                <target>Passwort geändert</target>
			</trans-unit>
            <trans-unit id="passwordManagement.change.success.body" xml:space="preserve">
				<source>Your password has been changed successfully.</source>
                <target>Ihr Passwort wurde erfolgreich geändert.</target>
			</trans-unit>

            <trans-unit id="passwordManagement.change.passwordNoMatch.title" xml:space="preserve">
				<source>Password no match</source>
                <target>Passwort keine Übereinstimmung</target>
			</trans-unit>
            <trans-unit id="passwordManagement.change.passwordNoMatch.body" xml:space="preserve">
				<source>The entered passwords do not match.</source>
                <target>Die eingegebenen Passwörter stimmen nicht überein.</target>
			</trans-unit>

            <trans-unit id="passwordManagement.change.currentPasswordInvalid.title" xml:space="preserve">
				<source>Current password invalid</source>
                <target>Bisheriges Passwort ungültig</target>
			</trans-unit>
            <trans-unit id="passwordManagement.change.currentPasswordInvalid.body" xml:space="preserve">
				<source>The given current password is invalid.</source>
                <target>Das angegebene bisherige Passwort ist ungültig.</target>
			</trans-unit>

            <trans-unit id="passwordManagement.reset.tokenInvalid.title" xml:space="preserve">
				<source>Failed to set password</source>
                <target>Passwort konnte nicht geändert werden</target>
			</trans-unit>
            <trans-unit id="passwordManagement.reset.tokenInvalid.body" xml:space="preserve">
				<source>Your password could not be reset due to an invalid token. Please use the password forget function to get a valid link.</source>
                <target>Aufgrund eines ungültigen Tokens konnte ihr Passwort nicht zurückgesetzt werden. Nutzen sie die Passwort vergessen Funktion erneut, um sich einen gültigen Link zuschicken zu lassen.</target>
			</trans-unit>

            <trans-unit id="passwordManagement.reset.passwordMissmatch.title" xml:space="preserve">
				<source>Passwords do not match</source>
                <target>Passwörter stimmen nicht überein</target>
			</trans-unit>
            <trans-unit id="passwordManagement.reset.passwordMissmatch.body" xml:space="preserve">
				<source>The given passwords do not match.</source>
                <target>Die angegebenen Passwörter stimmen nicht überein.</target>
			</trans-unit>

            <trans-unit id="passwordManagement.reset.loginFailed.title" xml:space="preserve">
				<source>Login failed</source>
                <target>Login fehlgeschlagen</target>
			</trans-unit>
            <trans-unit id="passwordManagement.reset.loginFailed.body" xml:space="preserve">
				<source>Failed to directly login account.</source>
                <target>Ihr Passwort wurde erfolgreich gesetzt. Der direkte Login ist allerdings fehlgeschlagen.</target>
			</trans-unit>
		</body>
	</file>
</xliff>
